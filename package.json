{
  "name": "monkey-react",
  "version": "1.3.2",
  "description": "A powerful yet simple React library designed to make responsive can adapt seamlessly to any device, providing a smooth, consistent user experience.",
  "author": "fuadlabibalh",
  "license": "MIT",
  "keywords": [
    "react-responsive",
    "react-component",
    "responsive-design",
    "css-generator",
    "react",
    "responsive",
    "latest"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./router": {
      "require": "./dist/cjs/router/index.js",
      "import": "./dist/esm/router/index.js"
    }
  },
  "scripts": {
    "dev": "vite ",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "rm -rf ./dist && npm run build:cjs && npm run build:esm"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "global": "^4.4.0",
    "globals": "^15.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "typescript": "^5.1.1",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fuadlabibalh/monkey-react.git"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"
  }
}
